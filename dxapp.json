{
  "name": "eggd_somalier_extract",
  "title": "Somalier extract",
  "summary": "Somalier extract",
  "dxapi": "1.0.0",
  "properties": {
    "githubRelease": "v1.0.0"
  },
  "inputSpec": [
    {
      "name": "input_file",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.vcf"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "out_file",
      "label": "file",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 2
      }
    },
    "interpreter": "bash",
    "release": "16.04",
    "distribution": "Ubuntu",
    "file": "src/eggd_somalier_extract.sh",
    "version": "1"
  },
  "access": {
    "project": "CONTRIBUTE",
    "allProjects": "VIEW",
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "aws:eu-central-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x2"
        }
      }
    }
  }
}
